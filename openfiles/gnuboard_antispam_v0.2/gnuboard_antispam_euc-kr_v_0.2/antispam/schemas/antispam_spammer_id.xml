<table name="antispam_spammer_id">
	<column name="user_id" type="varchar" size="255"/>
	<column name="user_ip" type="varchar" size="255"/>
	<column name="subject" type="text"/>
	<column name="content" type="text"/>
	<column name="spam_type" type="varchar" size="20"/>
	<column name="spam_score" type="number" size="11"/>
	<column name="datetime" type="varchar" size="19"/>
	<column name="manage" type="varchar" size="10"/>
	<column name="try_write_spam" type="number" size="11" default="0" />
</table>        
